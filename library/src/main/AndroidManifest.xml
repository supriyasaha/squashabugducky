<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		  package="com.squashabug.library">

	<application
		android:name="com.squashabug.library.DebugApplication"
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name="com.squashabug.library.MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>

				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>

		<activity
			android:name="com.squashabug.library.EventListActivity"
			android:parentActivityName="com.squashabug.library.MainActivity"/>

		<activity
			android:name="com.squashabug.library.DetailedEventActivity"
			android:parentActivityName="com.squashabug.library.EventListActivity"/>

		<activity
			android:name="com.squashabug.library.ApiLofListActivity"
			android:parentActivityName="com.squashabug.library.MainActivity"/>

		<activity
			android:name="com.squashabug.library.DetailedApiActivity"
			android:parentActivityName="com.squashabug.library.ApiLofListActivity"/>

		<service
			android:name="com.squashabug.library.DebugDuckyService"
			android:process=":remote">
			<intent-filter>
				<action android:name="com.example.coupondunia.couponduniadebugducky.DebugDuckyService"/>
			</intent-filter>
		</service>

		<receiver android:name="com.squashabug.library.TriggerActionReceiver">
			<intent-filter>
				<action android:name="com.example.coupondunia.couponduniadebugducky.yes"/>
			</intent-filter>
		</receiver>
	</application>

</manifest>
